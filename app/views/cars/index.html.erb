<h1>Cars</h1>

<table>
  <tr>
    <th>Manufacturer</th>
    <th>Model Name</th>
    <th>License Number</th>
  </tr>

  <% @cars.each do |car| %>
      <tr>
        <td><%= car.manufacturer %></td>
        <td><%= car.model %></td>
        <td><%= car.license_number %></td>
        <td><%= link_to 'Show', car_path(car) %></td>
      </tr>
  <% end %>
</table>
