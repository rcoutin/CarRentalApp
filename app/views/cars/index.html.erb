<h1>Cars</h1>

<table>
  <tr>
    <th>Manufacturer</th>
    <th>Model</th>
    <th>Style</th>
    <th>License Number</th>
    <th>Location</th>
    <th>Rate</th>
    <th>Status</th>
  </tr>

  <% @cars.each do |car| %>
    <tr>
      <td><%= car.manufacturer %></td>
      <td><%= car.model%></td>
      <td><%= car.style %></td>
      <td><%= car.license_number %></td>
      <td><%= car.location %></td>
      <td><%= car.rate %></td>
      <td><%= car.status %></td>
      <td><%= link_to 'Reserve', new_reservation_path(:car_id => car.id, :car_model => car.model) %></td>
      <td><%= link_to 'Show', car_path(car.id) %></td>
    </tr>
<% end %>
</table>

