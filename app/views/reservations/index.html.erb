<h1>Reservations</h1>

<table>
  <tr>
    <th>User ID</th>
    <th>Car ID</th>
    <th>Reserved From</th>
    <th>Reserved Till</th>
  </tr>

  <% @reservations.each do |reservation| %>
    <tr>
      <td><%= reservation.customer_id %></td>
      <td><%= reservation.car_id %></td>
      <td><%= reservation.from_time  %></td>
      <td><%= reservation.to_time %></td>
    </tr>
  <% end %>
</table>
