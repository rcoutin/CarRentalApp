<h1>Listing admins</h1>
<td><%= link_to 'Add', new_admin_path , :class => "btn btn-success"%></td>

<table>
  <tr>
  <th>First Name</th>
  <th>Last Name</th>
  <th>Role</th>
  </tr>

  <% @admins.each do |admin| %>
    <tr>
      <td><%= admin.first_name %></td>
      <td><%= admin.last_name %></td>

      <td><%= admin.is_super_admin%></td>
      <td><%= link_to 'Show', admin_path(admin) , :class => "btn btn-success"%></td>
    </tr>
  <% end %>
</table>
